<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">
    
    <!-- Outer circle (red/pink) -->
    <path
        android:pathData="M100,20 A80,80 0 1,1 100,180 A80,80 0 1,1 100,20 Z"
        android:fillColor="#E91E63"
        android:strokeWidth="0"/>
    
    <!-- Inner circle (white/transparent) to create the ring effect -->
    <path
        android:pathData="M100,50 A50,50 0 1,1 100,150 A50,50 0 1,1 100,50 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="0"/>
    
</vector>
